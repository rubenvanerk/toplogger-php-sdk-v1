{
    "name": "rubenvanerk/toplogger-php-sdk",
    "description": "PHP SDK for the TopLogger API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruben van Erk",
            "email": "contact@rubenvanerk.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^1.4",
        "spatie/data-transfer-object": "^3.8"
    },
    "require-dev": {
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "RubenVanErk\\TopLoggerPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RubenVanErk\\TopLoggerPhpSdk\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
